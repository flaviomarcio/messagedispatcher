git checkout master;
git submodule update --init;
git git pull origin master;
git submodule foreach git checkout master;
git submodule foreach git pull origin master;


git submodule foreach git git pull origin master;
git submodule foreach git submodule foreach git checkout master;
git submodule foreach git submodule foreach git pull origin master;

